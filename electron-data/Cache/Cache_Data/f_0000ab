export function ResearchAssistant() {
  _s();
  const [topic, setTopic] = useState("");
  const [years, setYears] = useState(2);
  const [maxPapers, setMaxPapers] = useState(30);
  const [loading, setLoading] = useState(false);
  const [progress, setProgress] = useState(null);
  const [steps, setSteps] = useState([]);
  const [result, setResult] = useState(null);
  const [error, setError] = useState(null);
  const [tasks, setTasks] = useState([]);
  const [expandedTasks, setExpandedTasks] = useState(/* @__PURE__ */ new Set());
  const [selectedPaper, setSelectedPaper] = useState(null);
  const [activeView, setActiveView] = useState("papers");
  const [pdfUrl, setPdfUrl] = useState(null);
  const wsRef = useRef(null);
  useEffect(() => {
    return () => {
      if (wsRef.current) {
        wsRef.current.close();
      }
    };
  }, []);
  const toggleTaskExpand = (taskId) => {
    setExpandedTasks((prev) => {
      const newSet = new Set(prev);
      if (newSet.has(taskId)) {
        newSet.delete(taskId);
      } else {
        newSet.add(taskId);
      }
      return newSet;
    });
  };
  const startResearch = async () => {
    if (!topic.trim()) return;
    setLoading(true);
    setError(null);
    setResult(null);
    setSteps([]);
    setTasks([]);
    setExpandedTasks(/* @__PURE__ */ new Set());
    setProgress({ state: "searching", progress: 0, task: "æ­£åœ¨åˆå§‹åŒ–..." });
    try {
      const wsUrl = "ws://localhost:8000/ws/research";
      console.log("Connecting to:", wsUrl);
      const ws = new WebSocket(wsUrl);
      wsRef.current = ws;
      ws.onopen = () => {
        console.log("WebSocket connected");
        ws.send(JSON.stringify({
          type: "start_research",
          topic: topic.trim(),
          years,
          max_papers: maxPapers
        }));
      };
      ws.onmessage = (event) => {
        const data = JSON.parse(event.data);
        if (data.type === "progress") {
          setProgress(data);
        } else if (data.type === "step") {
          setSteps((prev) => [...prev, {
            type: data.step_type,
            content: data.content,
            iteration: prev.length + 1,
            timestamp: (/* @__PURE__ */ new Date()).toISOString()
          }]);
        } else if (data.type === "task_list") {
          setTasks(data.tasks);
          setSteps((prev) => [...prev, {
            type: "thought",
            content: "å·²è§„åˆ’ç ”ç©¶ä»»åŠ¡åˆ—è¡¨",
            iteration: prev.length + 1,
            timestamp: (/* @__PURE__ */ new Date()).toISOString()
          }]);
        } else if (data.type === "task_update") {
          setTasks((prev) => prev.map(
            (task) => task.task_id === data.task_id ? { ...task, status: data.status } : task
          ));
        } else if (data.type === "task_step") {
          setTasks((prev) => prev.map((task) => {
            if (task.task_id === data.task_id) {
              return {
                ...task,
                steps: [...task.steps || [], {
                  step_type: data.step_type,
                  content: data.content,
                  timestamp: data.timestamp
                }]
              };
            }
            return task;
          }));
        } else if (data.type === "complete") {
          setResult(data.result);
          setSteps((prev) => [...prev, {
            type: "thought",
            content: "ç ”ç©¶å®Œæˆï¼",
            iteration: prev.length + 1,
            timestamp: (/* @__PURE__ */ new Date()).toISOString()
          }]);
          setLoading(false);
          ws.close();
        } else if (data.type === "error") {
          setError(data.error);
          setSteps((prev) => [...prev, {
            type: "observation",
            content: `é”™è¯¯: ${data.error}`,
            iteration: prev.length + 1,
            timestamp: (/* @__PURE__ */ new Date()).toISOString()
          }]);
          setLoading(false);
          ws.close();
        }
      };
      ws.onerror = (event) => {
        console.error("WebSocket error:", event);
        setError("WebSocketè¿žæŽ¥å¤±è´¥ï¼Œè¯·æ£€æŸ¥åŽç«¯æœåŠ¡æ˜¯å¦å¯åŠ¨");
        setLoading(false);
      };
      ws.onclose = (event) => {
        console.log("WebSocket closed:", event.code, event.reason);
      };
    } catch (e) {
      console.error("Error:", e);
      setError(e.message || "è¿žæŽ¥é”™è¯¯");
      setLoading(false);
    }
  };
