        const task = data.task;
        const message = {
          id: task.id,
          content: task.task,
          sender: "user",
          timestamp: new Date(task.created_at)
        };
        const answerMessage = {
          id: task.id + "-answer",
          content: task.answer || "",
          sender: "agent",
          timestamp: new Date(task.updated_at),
          steps: task.steps,
          isComplete: true,
          taskId: task.id
