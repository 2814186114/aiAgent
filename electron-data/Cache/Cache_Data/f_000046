    if (currentAnswer && currentSteps.length > 0 && !isProcessing) {
      const lastUserMessage = [...messages].reverse().find((m) => m.sender === "user");
      if (lastUserMessage) {
        const taskId = Date.now().toString();
        const agentMessage = {
          id: Date.now().toString(),
          content: currentAnswer,
