  const startResearch = async () => {
    if (!topic.trim()) return;
    setLoading(true);
    setError(null);
    setResult(null);
    setSteps([]);
    setProgress({ state: "searching", progress: 0, task: "æ­£åœ¨åˆå§‹åŒ–..." });
    const addStep = (type, content, iteration) => {
      setSteps((prev) => [...prev, {
        type,
        content,
        iteration,
        timestamp: (/* @__PURE__ */ new Date()).toISOString()
      }]);
    };
    try {
      const ws = new WebSocket("ws://localhost:8000/ws/research");
      wsRef.current = ws;
      ws.onmessage = (event) => {
        const data = JSON.parse(event.data);
        if (data.type === "progress") {
          setProgress(data);
          addStep("thought", data.task, steps.length + 1);
        } else if (data.type === "step") {
          addStep(data.step_type, data.content, steps.length + 1);
        } else if (data.type === "complete") {
          setResult(data.result);
          addStep("thought", "ç ”ç©¶å®Œæˆï¼", steps.length + 1);
          setLoading(false);
          ws.close();
        } else if (data.type === "error") {
          setError(data.error);
          addStep("observation", `é”™è¯¯: ${data.error}`, steps.length + 1);
          setLoading(false);
          ws.close();
        }
      };
      ws.onerror = () => {
        setError("WebSocketè¿žæŽ¥å¤±è´¥");
        setLoading(false);
      };
      ws.onopen = () => {
        ws.send(JSON.stringify({
          type: "start_research",
          topic: topic.trim(),
          years,
          max_papers: maxPapers
        }));
      };
    } catch (e) {
      setError(e.message);
      setLoading(false);
    }
